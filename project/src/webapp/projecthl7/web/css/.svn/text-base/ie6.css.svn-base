#login { 
	background: none !important;
	}

#content {
	width: 60%;
	margin: 40px 0 0 32px;
	float: left;
	}

#sidebar {
	float: right;
	margin: 40px 32px 0 0;
	background: url('../images/sidebar-header.jpg') top left repeat-x;
	width: 22%;
	}
	
#navtabs li a { 
    text-decoration: none;
    color: #ffffff;
    height: 39px;
    padding: 3px;
	}

#navtabs li a:hover {
	background: #f4f2e9;
	color: #5b5e56;
	}

#navtabs li#current { 
    background: url('../images/menu-tab-bg.jpg') bottom left repeat-x;
    color: #5b5e56; 
    padding: 0px 8px;
	}
	
#navtabs li#current a { 
	float: left;
	}